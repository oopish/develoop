{
  "name": "pegdev",
  "version": "0.0.8",
  "description": "A pleasant environment for developing the Oopish programming language",
  "main": "build/pegdev.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "usage"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
    "ace-editor-builds": "^1.2.0"
  },
  "keywords": [
    "CoffeeScript"
  ],
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "make": "node bin/akaybe-build",
    "index": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oopish/pegdev.git"
  },
  "author": "Rich Plastow <rich@richplastow.com> (http://richplastow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oopish/pegdev/issues"
  },
  "homepage": "http://pegdev.oopish.com/"
}
